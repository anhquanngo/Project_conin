                                <h6 class="card-subtitle">Simple table example</h6>
                                <table data-toggle="table" data-height="578" data-mobile-responsive="true"
                                    class="table-striped">
                                    <label>Search:<input type="search" class="form-control form-control-sm"
                                            placeholder="" aria-controls="file_export"></label>
                                    <thead>
                                        <tr>
                                            <th>User Id</th>
                                            <th>Role</th>
                                            <th>First Name</th>
                                            <th>Last Name</th>
                                            <th>Born</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>City</th>
                                            <th>Country</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% allUser.map((user, index)=>{ %>
                                        <tr id="tr-id-1" class="tr-class-1">
                                            <td><%= user.userId %></td>
                                            <td><%= user.role %></td>
                                            <td><%= user.identity.fName %></td>
                                            <td><%= user.identity.lName %></td>
                                            <% var born = new Date(user.identity.born) %>
                                            <td><%= born.getDate()+'-'+born.getMonth()+'-'+born.getFullYear()%>
                                            </td>
                                            <td><%= user.mail.email %></td>
                                            <td><%= user.identity.phone %></td>
                                            <td><%= user.identity.city %></td>
                                            <td><%= user.identity.country %></td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                                <nav aria-label="Page navigation example" style="padding-top: 15px;">
                                    <ul class="pagination justify-content-center">
                                        <% if (page > 1) { %>
                                        <li class="page-item">
                                            <a class="page-link" href="/user/?page=<%= page - 1 %>"
                                                aria-label="Previous">
                                                <span aria-hidden="true">&laquo;</span>
                                                <span class="sr-only">Previous</span>
                                            </a>
                                        </li>
                                        <% } %>
                                        <% range.map((i) => { %>
                                        <li class="page-item <%= page === i ? "active" : "" %>"></li>
                                        <% if (i === "...") { %>
                                        <span> <%= i %> </span>
                                        <% } else { %>
                                        <a class="page-link" href="/user/?page=<%= i %>"><%= i %></a>
                                        <% } %>
                                        </li>
                                        <% }) %>
                                        <% if (page < totalPages) { %>
                                        <li class="page-item ">
                                            <a class="page-link" href="/user/?page=<%= page + 1 %>"> Next </a>
